{
  "id": "/pentaho/biserver6",
  "cpus": 1,
  "mem": 2048.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "pentaho/biserver:6.0",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 8080, "hostPort": 0, "servicePort": 8081, "protocol": "tcp" }
      ]
    }
  },
  "constraints": [["hostname", "UNIQUE"]],
  "healthChecks": [{
          "protocol": "HTTP",
          "portIndex": 0,
          "path": "/",
          "gracePeriodSeconds": 300,
          "intervalSeconds": 20,
          "maxConsecutiveFailures": 3
  }]
}
